train_net: "models/gtsdb/faster_rcnn_end2end/train_new.prototxt"
base_lr: 0.001
# Other lr_policy which can be used are multistep, fixed, inv, poly etc
lr_policy: "step"
gamma: 0.1
stepsize: 5000
display: 1
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "gtsdb_30_12_lr_0.001_locip1_4096"
iter_size: 10
