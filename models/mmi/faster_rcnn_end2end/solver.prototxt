train_net: "models/mmi/faster_rcnn_end2end/train.prototxt"
base_lr: 0.005
# Other lr_policy which can be used are multistep, fixed, inv, poly etc
lr_policy: "step"
gamma: 0.1
stepsize: 5000
display: 20
average_loss: 100
# iter_size: 1
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "mmi_15_12_lr_0.005"
iter_size: 10
